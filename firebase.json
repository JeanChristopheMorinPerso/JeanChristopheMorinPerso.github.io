{
  "hosting": {
    "public": "public",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/package.json",
      "**/yarn.lock"
    ],
    "headers": [
      {
        "source": "**/*.(jpg|png|svg)",
        "headers": [
          {
            "key": "Cache-Control",
            "value": "max-age=3600"
          }
        ]
      }
    ]
  }
}
